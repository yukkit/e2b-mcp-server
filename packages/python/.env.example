# E2B MCP Server Configuration

# Environment variables for production deployment

# Logging
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL

# Sandbox Management
MAX_ACTIVE_SANDBOXES=10  # Maximum number of concurrent sandboxes

# E2B API Configuration
# E2B_API_KEY=your_api_key_here  # Set your E2B API key

# Timeout Settings (in milliseconds)
# DEFAULT_SANDBOX_TIMEOUT_MS=300000  # Default: 5 minutes
# MAX_SANDBOX_TIMEOUT_MS=3600000     # Maximum: 1 hour
